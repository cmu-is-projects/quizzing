<div id= "division1" class="row">
	<div class="col s8">
	<div class="card">
	  <div class="card-content black-text">
		<h5>All Students</h5>
		    <div class="divider"></div>
		      <table class="responsive-table">
		        <thead>
		          <tr>
		            <th>Name</th>
		            <th>Team</th>
		            <th>Grade</th>
		          </tr>
		        </thead>
		        <tbody>
		        	<% count=0 %> 
		          	<% @juniors.each do |student| %>
		          	<% if count <10 %>
			            <tr>
			              <td><%= link_to student.proper_name, student_path(student) %></td>
			              <td><%= link_to student.current_team.name, team_path(student.current_team) %></td>
			              <td><%=student.grade%></td>
			            </tr>
	            	<% else %>
			            <tr class="more_junior_students">
			              <td><%= link_to student.proper_name, student_path(student) %></td>
			              <td><%= link_to student.current_team.name, team_path(student.current_team) %></td>
			              <td><%=student.grade%></td>
			            </tr>
		         	<% end %>
		         	<% count += 1 %> 
		         	<%end %>
					<% if @juniors.length >= 10 %> 
						<tr>
						  <td></td>
						  <td></td>
						  <td style="text-align: right;"><a class="more_junior_students" href="#"> More <i class="fa fa-angle-double-down"></i></a></td>
						</tr>
					<% end %>
		        </tbody>
		      </table>
		      <br>
		  </div>
		</div>
	</div>

	<div class="col s4">
		<div class="card">
	  		<div class="card-content black-text">
  				<h5><i id="top-team-star-icon"class="fa fa-star-o"></i> Top Students</h5>
		    <div class="divider"></div>
		      <table class="responsive-table">
		        <thead>
		          <tr>
		            <th data-field="id">Rank</th>
		            <th data-field="name">Student</th>
		            <th data-field="price">Score</th>
		          </tr>
		        </thead>
		        <tbody>
		          <% @junior_standings.each do |standing| %>
		            <tr>
		              <td><%=standing.position %></td>
		              <td><%= link_to standing.student.proper_name, student_path(standing.student) %></td>
		              <td><%=standing.total_points %></td>
		            </tr>
		          <% end %>
		        </tbody>
		      </table>
	      	<br>
			<div align="right">
				<%= link_to indiv_standings_path  do %>
					More <i class="fa fa-angle-double-right"></i>
				<% end %>
			</div>
		  </div>
		</div>
	</div>
</div>

