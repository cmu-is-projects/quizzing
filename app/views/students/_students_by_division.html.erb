<%# @year_div_quizzers = YearQuizzer.get_all_quizzers_for_division_for_year(division) %>
<% @students_by_div = Team.where(division: div).map{|t| t.current_students}.uniq.flatten! %>
  <%# unless @year_div_quizzers.empty? %>
  <% unless @students_by_div.empty? %>
    <h4><%= "#{div.name} Teams" %></h4>

    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Team</th>
<!--           <th>Division</th> -->
        </tr>
      </thead>

      <tbody>
        <% @students_by_div.each do |student| %>
          <tr>
            <td><%= link_to student.name, student_path(student) %></td>
            <td><%= student.current_team.name %></td>
<!--             <td><%= student.current_team.division.name %></td> -->
    	      <% if current_user.role == "coach" && current_user.coach.organization_id == student.current_organization.id %>
              <td><%= link_to 'Edit', edit_student_path(student), class: "button tiny radius" %></td>
    	      <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  <% else %>
    <div class="panel clearfix">
      <h3><%= "No #{division.name} Teams " %></h3>
    </div>
  <% end %>

