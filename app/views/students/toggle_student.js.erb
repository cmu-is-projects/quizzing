<% if @changed == 1 %>
$('div#students').replaceWith('<%= j render(:partial => "dashboards/students", :locals => {:student_type => @junior_students}) %>');
<% elsif @changed == 2 %>
$('div#students').replaceWith('<%= j render(:partial => "dashboards/students", :locals => {:student_type => @senior_students}) %>');
<% else %>
$('div#students').replaceWith('<%= j render(:partial => "dashboards/students", :locals => {:student_type => @seniorb_students}) %>');
<% end %>