<% if @changed == 1 %>
$('div#junior_students').replaceWith('<%= j render(:partial => "dashboards/junior_students", :locals => {:student_type => @junior_students}) %>');
<% elsif @changed == 2 %>
$('div#senior_students').replaceWith('<%= j render(:partial => "dashboards/senior_students", :locals => {:student_type => @senior_students}) %>');
<% else %>
$('div#seniorb_students').replaceWith('<%= j render(:partial => "dashboards/seniorb_students", :locals => {:student_type => @seniorb_students}) %>');
<% end %>