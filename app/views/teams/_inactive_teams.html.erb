<% if current_user.role == "admin" %>
  <% unless @inactive_teams.empty? %>
    <br /><br /><br />
    <div class="panel clearfix">
      <h5>Inactive Teams</h5>
      <ul>
        <% @inactive_teams.each do |team| %>
          <li><%= link_to team.name, team_path(team) %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
<% end %>
