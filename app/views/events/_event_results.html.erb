<div class="row">
   <% @event_div_teams = EventTeam.get_all_teams_for_event_and_division(@event, division) %>
<% unless @event_div_teams.empty? %> 
    <div class="col s12"> 
      <ul class="tabs">
        <li class="tab col s2"><a class="active" href="#division1">Junior</a></li>
        <li class="tab col s2"><a href="#division2">Senior</a></li>
        <li class="tab col s2"><a href="#division3">Senior B</a></li>
      </ul>
    </div>
    <div id="division1" class="col s12">
      <div class="row">
              <div class="col 12 m12">
                <div class="card blue-grey lighten-5">
                  <div class="card-content black-text">
                      <table class="centered">
                       <thead>
                          <tr>
                            <th>Rank</th>
                            <th>Team</th>
                            <th>Total Points</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% @junior_team_results.each do |j| %>
                            <tr>
                              <td><%= j.position %></td>
                              <td><%= j.team.name %></td>
                              <td><%= j.total_points %></td>
                            </tr>
                          <% end %>
                            <tr>
                              <td></td>
                              <td></td>
                              <td align="left"><%= link_to team_standings_path  do %> 
                              More <i class="fa fa-angle-double-right"></i>
                              <% end %></td>
                            </tr>
                        </tbody>
                      </table>
                  </div>
                </div>
              </div>
            </div>

    </div>

