<!-- Event Details -->
<h3>Event Details</h3>
  <strong>Start date:</strong>
  <%= @event.start_date.strftime('%B %d, %Y') %> <br /><br />

  <strong>End date:</strong>
  <%= @event.end_date.strftime('%B %d, %Y') %> <br /><br />

  <strong>Start time:</strong>
  <%= @event.start_time.strftime('%I:%M %p') %> <br /><br />

  <strong>Number of rounds:</strong>
  <%= @event.num_rounds %> <br /><br />

  <strong>Location:</strong>
  <%= @event.organization.name %> <br /><br />

<%= link_to 'Edit', edit_event_path(@event), class: "button tiny radius" %>
<%= link_to 'Back', events_path, class: 'button tiny secondary radius' %>
