<div id="students">
  <div class="card blue-grey lighten-5">
    <div class="subtitle card-content black-text">
      <span class="card-title black-text">
        <div class="row">
          <div class="col m8">Teams</div>
          <div class="col m4">
            <p class="subtitle"> Total: <%=team_type.size%> </p> 
          </div>
        </div>
      </span>
      <div class="divider"></div>
      <table class="center">
        <thead>
          <th>Name</th>
          <th>Division</th>
          <th>Active</th>
        </thead>
        <tbody>
          <% team_type.each do |t| %>
            <tr>
              <td><%=t.name%></td>
              <td><%=t.division.name%></td>
            </tr>
          <%end%>
        </tbody>
      </table>
    </div>
  </div>
</div>