<div class="container">
<h3> Welcome, <%= @coach.proper_name %> </h3> 
  <div class="row">
    <div class="col s12">
      <ul class="tabs">
        <li class="tab col s2"><a class="active" href="#division1">Junior</a></li>
        <li class="tab col s2"><a href="#division2">Senior </a></li>
        <li class="tab col s2"><a href="#division3">Senior B</a></li>
      </ul>
    </div>
  </div>

    <div id="division1" class="col s12">
      <div class="row">
        <!-- Junior Teams Column -->
        <div class="col 6 m6" >
          <%= render(:partial => "junior_team_cards")%>
        </div>

        <!-- Stats Column -->
        <div class="col 6 m6">
          <%= render(:partial => "top_students", :locals => {:top_standings => @top_juniors})%>
          <%= render(:partial => "most_improved", :locals => {:most_improved => @improved_juniors})%>
          <%#= render(:partial => "needs_love")%>
        </div>
      </div>
    </div>

    <div id="division2" class="col s12">
      <div class="row">
       <!-- Senior Teams Column -->
        <div class="col 6 m6">
          <%= render(:partial => "senior_team_cards")%>
        </div>
      <!-- Stats Column -->
        <div class="col 6 m6">
          <%= render(:partial => "top_students", :locals => {:top_standings => @top_seniors})%>
          <%= render(:partial => "most_improved", :locals => {:most_improved => @improved_seniors})%>
          <%#= render(:partial => "needs_love")%>
        </div>
      </div>
    </div>

    <div id="division3" class="col s12">
      <div class="row">
       <!-- Senior Teams Column -->
        <div class="col 6 m6">
          <%= render(:partial => "seniorb_team_cards")%>
        </div>
     
      <!-- Stats Column -->
        <div class="col 6 m6">
         <%= render(:partial => "top_students", :locals => {:top_standings => @top_seniorb})%>
         <%= render(:partial => "most_improved", :locals => {:most_improved => @improved_seniorb})%>
         <%#= render(:partial => "needs_love")%>
        </div>
      </div>
    </div>

</div>